{
  "name": "geekplanet",
  "version": "1.4.0",
  "description": "A Webshop built with React/Redux, Node JS and Mongo DB",
  "repository": "https://github.com/r3dDoX/geekplanet",
  "engines": {
    "node": "9.2.1"
  },
  "scripts": {
    "predist": "npm run build",
    "flow:install": "flow-typed install --overwrite",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "test": "npm run lint && jest",
    "test:docker": "docker run --rm -it -v \"$(pwd)\":/usr/app:cached -w /usr/app r3ddox/node-pdftk:9.2.1 npm test",
    "test:watch": "jest --watch",
    "test:watch:docker": "docker run --rm -it -v \"$(pwd)\":/usr/app -w /usr/app r3ddox/node-pdftk:9.2.1 npm run test:watch",
    "build": "NODE_ENV='production' webpack --config webpack.prod.config.js --progress --optimize-minimize -p",
    "build:production": "NODE_ENV='production' CONFIG='production' npm run build",
    "build:staging": "CONFIG='staging' npm run build",
    "build:docker": "docker run --rm -it -v \"$(pwd)\":/usr/app:cached -w /usr/app r3ddox/node-pdftk:9.2.1 npm run build:production",
    "start": "NODE_ENV='production' node src/server/app.js",
    "dev": "NODE_ENV='development' webpack-dev-server --color --config webpack.dev.config.js & nodemon -e js,jsx,json --watch ./src/common --watch ./src/config --watch ./src/server src/server/app.js",
    "debug": "NODE_ENV='development' webpack-dev-server --color --config webpack.dev.config.js & nodemon -e js,jsx,json --watch ./src/common --watch ./src/config --watch ./src/server --inspect-brk=0.0.0.0:9229 -e js,jsx,json src/server/app.js"
  },
  "author": "Patrick Walther",
  "license": "ISC",
  "dependencies": {
    "babel-core": "6.26.0",
    "body-parser": "1.18.2",
    "chalk": "2.3.0",
    "compression": "1.7.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "express": "4.16.0",
    "express-jwt": "5.3.0",
    "express-mongo-sanitize": "1.3.2",
    "gridfs-stream": "1.1.1",
    "jwks-rsa": "1.2.1",
    "mime-types": "2.1.17",
    "mongoose": "4.13.8",
    "mongoose-sequence": "4.0.1",
    "multer": "1.3.0",
    "newrelic": "2.5.0",
    "nodemailer": "4.4.1",
    "pdffiller": "0.0.10",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-intl": "2.4.0",
    "sharp": "0.18.4",
    "shortid": "2.2.8",
    "streamifier": "0.1.1",
    "stripe": "5.4.0"
  },
  "devDependencies": {
    "auth0-js": "9.0.2",
    "babel-eslint": "8.2.1",
    "babel-jest": "22.0.4",
    "babel-loader": "7.1.2",
    "babel-plugin-dynamic-import-webpack": "1.0.2",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.12",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "compression-webpack-plugin": "1.1.3",
    "copy-webpack-plugin": "4.3.1",
    "css-loader": "0.28.8",
    "eslint": "4.14.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.5.1",
    "flow-bin": "0.62.0",
    "flow-typed": "2.2.3",
    "html-webpack-plugin": "2.30.1",
    "jest": "22.0.4",
    "jwt-decode": "2.2.0",
    "material-ui": "0.20.0",
    "nodemon": "1.14.8",
    "prop-types": "15.6.0",
    "react-animate-height": "0.10.9",
    "react-countup": "3.0.2",
    "react-drafts": "1.0.2",
    "react-dragula": "1.1.17",
    "react-helmet": "5.2.0",
    "react-hot-loader": "3.1.3",
    "react-image-gallery": "0.8.6",
    "react-infinite-scroller": "1.1.2",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-test-renderer": "16.2.0",
    "redux": "3.7.2",
    "redux-form": "7.2.0",
    "redux-thunk": "2.2.0",
    "style-loader": "0.19.1",
    "styled-components": "2.4.0",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.10.0",
    "webpack-merge": "4.1.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/test/localStorage.mock.js",
    "setupFiles": [
      "./src/test/jest.setup.js"
    ]
  }
}

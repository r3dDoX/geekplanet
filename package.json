{
  "name": "geekplanet",
  "version": "1.2.0",
  "description": "A Webshop built with React/Redux, Node JS and Mongo DB",
  "repository": "https://github.com/r3dDoX/geekplanet",
  "engines": {
    "node": "8.6.0"
  },
  "scripts": {
    "greenkeeper_update": "greenkeeper-lockfile-update",
    "greenkeeper_upload": "greenkeeper-lockfile-upload",
    "predist": "npm run build",
    "dist": "bestzip geekplanet.zip dist/ src/ package.json webpack.prod.config.js",
    "flow:install": "flow-typed install --overwrite",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "test": "npm run lint && jest",
    "test:docker": "docker run --rm -it -v \"$(pwd)\":/usr/app:cached -w /usr/app r3ddox/node-pdftk:8.6.0 npm test",
    "test:watch": "jest --watch",
    "test:watch:docker": "docker run --rm -it -v \"$(pwd)\":/usr/app -w /usr/app r3ddox/node-pdftk:8.6.0 npm run test:watch",
    "build": "NODE_ENV='production' webpack --config webpack.prod.config.js --progress --optimize-minimize -p",
    "build:production": "NODE_ENV='production' CONFIG='production' npm run build",
    "build:staging": "CONFIG='staging' npm run build",
    "build:docker": "docker run --rm -it -v \"$(pwd)\":/usr/app:cached -w /usr/app r3ddox/node-pdftk:8.6.0 npm run build:production",
    "heroku-postbuild": "npm run build",
    "start": "NODE_ENV='production' node src/server/app.js",
    "dev": "NODE_ENV='development' webpack-dev-server --color --config webpack.dev.config.js & nodemon -e js,jsx,json --watch ./src/common --watch ./src/config --watch ./src/server src/server/app.js",
    "debug": "NODE_ENV='development' webpack-dev-server --color --config webpack.dev.config.js & nodemon -e js,jsx,json --watch ./src/common --watch ./src/config --watch ./src/server --inspect-brk=0.0.0.0:9229 -e js,jsx,json src/server/app.js"
  },
  "author": "Patrick Walther",
  "license": "ISC",
  "dependencies": {
    "babel-core": "6.26.0",
    "body-parser": "1.18.2",
    "chalk": "2.1.0",
    "compression": "1.7.1",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.2",
    "express": "4.16.0",
    "express-jwt": "5.3.0",
    "express-mongo-sanitize": "1.3.2",
    "gridfs-stream": "1.1.1",
    "jwks-rsa": "1.2.0",
    "mime-types": "2.1.17",
    "mongoose": "4.12.3",
    "mongoose-sequence": "4.0.1",
    "multer": "1.3.0",
    "newrelic": "2.3.0",
    "nodemailer": "4.2.0",
    "pdffiller": "0.0.10",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-intl": "2.4.0",
    "sharp": "0.18.4",
    "shortid": "2.2.8",
    "streamifier": "0.1.1",
    "stripe": "5.1.1"
  },
  "devDependencies": {
    "auth0-js": "8.10.1",
    "babel-eslint": "8.0.1",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-dynamic-import-webpack": "1.0.1",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.10",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "bestzip": "1.1.4",
    "compression-webpack-plugin": "1.0.1",
    "copy-webpack-plugin": "4.1.1",
    "css-loader": "0.28.7",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "flow-bin": "0.57.3",
    "flow-typed": "2.2.0",
    "greenkeeper-lockfile": "1.9.0",
    "html-webpack-plugin": "2.30.1",
    "jest": "21.2.1",
    "jwt-decode": "2.2.0",
    "material-ui": "0.19.4",
    "nodemon": "1.12.1",
    "prop-types": "15.6.0",
    "react-addons-test-utils": "15.6.2",
    "react-animate-height": "0.10.5",
    "react-countup": "2.3.0",
    "react-document-title": "2.0.3",
    "react-drafts": "1.0.2",
    "react-dragula": "1.1.17",
    "react-hot-loader": "3.1.1",
    "react-image-gallery": "0.8.3",
    "react-infinite-scroller": "1.0.15",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-tap-event-plugin": "3.0.2",
    "react-test-renderer": "16.0.0",
    "redux": "3.7.2",
    "redux-form": "7.1.1",
    "redux-thunk": "2.2.0",
    "style-loader": "0.19.0",
    "styled-components": "2.2.1",
    "webpack": "3.7.1",
    "webpack-dev-server": "2.9.2",
    "webpack-merge": "4.1.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/test/localStorage.mock.js",
    "setupFiles": [
      "./src/test/jest.setup.js"
    ]
  }
}

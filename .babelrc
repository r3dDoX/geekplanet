{
  "env": {
    "development": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false,
            "targets": {
              "browsers": [
                "last 2 versions"
              ]
            }
          }
        ],
        "@babel/preset-react"
      ],
      "plugins": [
        "react-hot-loader/babel"
      ]
    },
    "production": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "browsers": [
                "last 2 versions"
              ]
            }
          }
        ],
        "@babel/preset-react"
      ],
      "plugins": [
        [
          "transform-imports",
          {
            "redux-form": {
              "transform": "redux-form/es/${member}",
              "preventFullImport": true
            }
          }
        ]
      ]
    },
    "test": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
      ],
      "plugins": [
        "babel-plugin-dynamic-import-node"
      ]
    }
  },
  "plugins": [
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    "transform-react-remove-prop-types",
    [
      "react-intl",
      {
        "messagesDir": "./dist/messages",
        "enforceDescriptions": false
      }
    ]
  ]
}
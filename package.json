{
  "name": "geekplanet",
  "version": "1.0.0",
  "description": "A Webshop built with React/Redux, Node JS and Mongo DB",
  "engines": {
    "node": ">=7.2.0"
  },
  "scripts": {
    "predist": "npm run build",
    "dist": "bestzip geekplanet.zip dist/ src/ package.json webpack.prod.config.js",
    "flow:install": "flow-typed install --overwrite",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "test": "npm run lint && jest",
    "test:docker": "docker run --rm -it -v \"$(pwd)\":/usr/app:cached -w /usr/app r3ddox/node-pdftk npm test",
    "test:watch": "jest --watch",
    "test:watch:docker": "docker run --rm -it -v \"$(pwd)\":/usr/app:cached -w /usr/app r3ddox/node-pdftk npm run test:watch",
    "build": "NODE_ENV='production' webpack --config webpack.prod.config.js --progress --optimize-minimize -p",
    "build:production": "CONFIG='production' npm run build",
    "build:staging": "CONFIG='staging' npm run build",
    "heroku-postbuild": "npm run build",
    "start": "NODE_ENV='production' node src/server/app.js",
    "dev": "NODE_ENV='development' webpack-dev-server --color --config webpack.dev.config.js & nodemon src/server/app.js"
  },
  "author": "Patrick Walther",
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.17.2",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "express": "4.15.2",
    "express-jwt": "5.3.0",
    "express-mongo-sanitize": "1.3.2",
    "mongoose": "4.10.2",
    "mongoose-gridfs": "0.1.0",
    "mongoose-sequence": "3.2.0",
    "multer": "1.3.0",
    "nodemailer": "4.0.1",
    "pdffiller": "0.0.10",
    "shortid": "2.2.8",
    "streamifier": "0.1.1",
    "stripe": "4.22.0"
  },
  "devDependencies": {
    "auth0-lock": "10.16.0",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.5",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.5.1",
    "babel-preset-react": "6.24.1",
    "bestzip": "1.1.4",
    "copy-webpack-plugin": "4.0.1",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "flow-bin": "0.47.0",
    "flow-typed": "2.1.2",
    "html-webpack-plugin": "2.28.0",
    "jest": "20.0.4",
    "jwt-decode": "2.2.0",
    "material-ui": "0.18.1",
    "nodemon": "1.11.0",
    "prop-types": "15.5.10",
    "react": "15.5.4",
    "react-addons-test-utils": "15.5.1",
    "react-dom": "15.5.4",
    "react-hot-loader": "3.0.0-beta.6",
    "react-intl": "2.3.0",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.1",
    "react-svg-inline-loader": "0.2.2",
    "react-tap-event-plugin": "2.0.1",
    "react-test-renderer": "15.5.4",
    "redux": "3.6.0",
    "redux-form": "6.7.0",
    "redux-thunk": "2.2.0",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5",
    "webpack-merge": "4.1.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/test/localStorage.mock.js"
  }
}

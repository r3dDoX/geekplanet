{
  "name": "geekplanet",
  "version": "2.0.3",
  "description": "A Webshop built with React/Redux, Node JS and Mongo DB",
  "repository": "https://github.com/r3dDoX/geekplanet",
  "engines": {
    "node": "9.9.0"
  },
  "scripts": {
    "predist": "npm run build",
    "flow:install": "flow-typed install --overwrite",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "test": "npm run lint && jest",
    "test:docker": "docker run --rm -it -v \"$(pwd)\":/usr/app:delegated -w /usr/app r3ddox/node-pdftk:10.13.0 npm test",
    "test:watch": "jest --watch",
    "test:watch:docker": "docker run --rm -it -v \"$(pwd)\":/usr/app:delegated -w /usr/app r3ddox/node-pdftk:10.13.0 npm run test:watch",
    "build": "NODE_ENV='production' webpack --config webpack.prod.config.js --progress --optimize-minimize -p",
    "build:production": "NODE_ENV='production' CONFIG='production' npm run build",
    "build:staging": "CONFIG='staging' npm run build",
    "build:docker": "docker run --rm -it -v \"$(pwd)\":/usr/app:cached -w /usr/app r3ddox/node-pdftk:10.13.0 npm run build:production",
    "start": "NODE_ENV='production' node src/server/app.js",
    "dev": "NODE_ENV='development' webpack-dev-server --color --config webpack.dev.config.js & nodemon -e js,jsx,json --watch ./src/common --watch ./src/config --watch ./src/server src/server/app.js",
    "debug": "NODE_ENV='development' webpack-dev-server --color --config webpack.dev.config.js & nodemon -e js,jsx,json --watch ./src/common --watch ./src/config --watch ./src/server --inspect-brk=0.0.0.0:9229 -e js,jsx,json src/server/app.js"
  },
  "author": "Patrick Walther",
  "license": "ISC",
  "dependencies": {
    "babel-core": "6.26.3",
    "body-parser": "1.18.3",
    "chalk": "2.4.1",
    "compression": "1.7.3",
    "coupon-code": "0.4.5",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "express": "4.16.3",
    "express-async-handler": "1.1.4",
    "express-jwt": "5.3.1",
    "express-mongo-sanitize": "1.3.2",
    "gridfs-stream": "1.1.1",
    "jwks-rsa": "1.3.0",
    "mime-types": "2.1.21",
    "mongoose": "5.3.12",
    "mongoose-sequence": "4.0.1",
    "multer": "1.4.1",
    "newrelic": "4.11.0",
    "nodemailer": "4.6.8",
    "pdffiller": "0.0.10",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-intl": "2.7.1",
    "sharp": "0.21.0",
    "shortid": "2.2.14",
    "streamifier": "0.1.1",
    "stripe": "6.15.0"
  },
  "devDependencies": {
    "@material-ui/core": "3.5.1",
    "@material-ui/icons": "3.0.1",
    "auth0-js": "9.8.2",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.0.1",
    "babel-loader": "7.1.5",
    "babel-plugin-dynamic-import-node": "2.1.0",
    "babel-plugin-dynamic-import-webpack": "1.0.2",
    "babel-plugin-react-intl": "3.0.1",
    "babel-plugin-transform-imports": "1.5.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.18",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "compression-webpack-plugin": "2.0.0",
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "1.0.1",
    "downshift": "3.1.5",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "flow-bin": "0.86.0",
    "flow-typed": "2.5.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.1.0",
    "jwt-decode": "2.2.0",
    "keycode": "2.2.0",
    "mockingoose": "2.11.0",
    "node-mocks-http": "1.7.3",
    "nodemon": "1.18.6",
    "prop-types": "15.6.2",
    "query-string": "^6.2.0",
    "react-countup": "3.0.3",
    "react-drafts": "1.0.3",
    "react-dragula": "1.1.17",
    "react-helmet": "5.2.0",
    "react-hot-loader": "4.3.11",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-slick": "0.23.1",
    "react-test-renderer": "16.5.2",
    "redux": "4.0.0",
    "redux-form": "7.4.2",
    "redux-thunk": "2.3.0",
    "style-loader": "0.23.1",
    "styled-components": "4.1.1",
    "webpack": "4.25.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10",
    "webpack-merge": "4.1.4"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/test/localStorage.mock.js",
    "setupFiles": [
      "./src/test/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(auth0-js|redux-form))"
    ]
  }
}

{
  "name": "geekplanet",
  "version": "1.0.0",
  "description": "A Webshop built with React/Redux, Node JS and Mongo DB",
  "engines": {
    "node": ">=7.2.0"
  },
  "scripts": {
    "predist": "npm run build",
    "dist": "bestzip geekplanet.zip dist/ src/ package.json webpack.prod.config.js",
    "flow:install": "flow-typed install",
    "test": "eslint \"src/**/*.{js,jsx}\"",
    "build": "NODE_ENV='production' webpack --config webpack.prod.config.js --progress --optimize-minimize -p",
    "heroku-postbuild": "npm run build",
    "start": "NODE_ENV='production' node src/server/app.js",
    "dev": "webpack-dev-server --config webpack.dev.config.js & nodemon src/server/app.js"
  },
  "author": "Patrick Walther",
  "license": "ISC",
  "dependencies": {
    "compression": "1.6.2",
    "express": "5.0.0-alpha.4",
    "express-jwt": "5.1.0",
    "express-mongo-sanitize": "1.3.1",
    "mongoose": "4.8.5",
    "mongoose-gridfs": "0.1.0",
    "multer": "1.3.0",
    "shortid": "2.2.6",
    "streamifier": "0.1.1"
  },
  "devDependencies": {
    "auth0-lock": "10.12.0",
    "babel-core": "6.23.0",
    "babel-loader": "6.3.2",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-transform-flow-strip-types": "6.21.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-plugin-transform-react-jsx": "6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "0.3.2",
    "babel-plugin-transform-runtime": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.16.0",
    "bestzip": "1.1.3",
    "copy-webpack-plugin": "4.0.1",
    "eslint": "3.16.1",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.0",
    "flow-bin": "0.38.0",
    "flow-typed": "2.0.0",
    "html-webpack-plugin": "2.28.0",
    "jwt-decode": "2.1.0",
    "material-ui": "0.17.0",
    "nodemon": "1.11.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-intl": "2.2.3",
    "react-redux": "5.0.3",
    "react-router": "3.0.2",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.6.0",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.4.1"
  }
}

{
  "name": "geekplanet",
  "version": "1.0.0",
  "description": "A Webshop built with React/Redux, Node JS and Mongo DB",
  "repository": "https://github.com/r3dDoX/geekplanet",
  "engines": {
    "node": "8.4.0"
  },
  "scripts": {
    "greenkeeper_update": "greenkeeper-lockfile-update",
    "greenkeeper_upload": "greenkeeper-lockfile-upload",
    "predist": "npm run build",
    "dist": "bestzip geekplanet.zip dist/ src/ package.json webpack.prod.config.js",
    "flow:install": "flow-typed install --overwrite",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "test": "npm run lint && jest",
    "test:docker": "docker run --rm -it -v \"$(pwd)\":/usr/app:cached -w /usr/app r3ddox/node-pdftk:8.4.0 npm test",
    "test:watch": "jest --watch",
    "test:watch:docker": "docker run --rm -it -v \"$(pwd)\":/usr/app -w /usr/app r3ddox/node-pdftk:8.4.0 npm run test:watch",
    "build": "NODE_ENV='production' webpack --config webpack.prod.config.js --progress --optimize-minimize -p",
    "build:production": "NODE_ENV='production' CONFIG='production' npm run build",
    "build:staging": "CONFIG='staging' npm run build",
    "build:docker": "docker run --rm -it -v \"$(pwd)\":/usr/app:cached -w /usr/app r3ddox/node-pdftk:8.4.0 npm run build:production",
    "heroku-postbuild": "npm run build",
    "start": "NODE_ENV='production' node src/server/app.js",
    "dev": "NODE_ENV='development' webpack-dev-server --color --config webpack.dev.config.js & nodemon -e js,jsx,json --watch ./src/common --watch ./src/config --watch ./src/server src/server/app.js",
    "debug": "NODE_ENV='development' webpack-dev-server --color --config webpack.dev.config.js & nodemon -e js,jsx,json --watch ./src/common --watch ./src/config --watch ./src/server --inspect-brk=0.0.0.0:9229 -e js,jsx,json src/server/app.js"
  },
  "author": "Patrick Walther",
  "license": "ISC",
  "dependencies": {
    "babel-core": "6.26.0",
    "body-parser": "1.17.2",
    "chalk": "2.1.0",
    "compression": "1.7.0",
    "express": "4.15.3",
    "express-jwt": "5.3.0",
    "express-mongo-sanitize": "1.3.2",
    "gridfs-stream": "1.1.1",
    "jwks-rsa": "1.2.0",
    "mime-types": "2.1.16",
    "mongoose": "4.11.7",
    "mongoose-sequence": "4.0.0",
    "multer": "1.3.0",
    "nodemailer": "4.0.1",
    "pdffiller": "0.0.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-intl": "2.3.0",
    "sharp": "0.18.2",
    "shortid": "2.2.8",
    "streamifier": "0.1.1",
    "stripe": "4.24.0"
  },
  "devDependencies": {
    "auth0-js": "8.9.3",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-dynamic-import-webpack": "1.0.1",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.8",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "bestzip": "1.1.4",
    "compression-webpack-plugin": "1.0.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.28.5",
    "enzyme": "2.9.1",
    "eslint": "4.5.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.2.1",
    "flow-bin": "0.53.1",
    "flow-typed": "2.1.5",
    "greenkeeper-lockfile": "1.8.1",
    "html-webpack-plugin": "2.30.1",
    "jest": "20.0.4",
    "jwt-decode": "2.2.0",
    "material-ui": "0.19.0",
    "nodemon": "1.11.0",
    "prop-types": "15.5.10",
    "react-addons-test-utils": "15.6.0",
    "react-animate-height": "0.10.0",
    "react-hot-loader": "3.0.0-beta.6",
    "react-image-gallery": "0.8.3",
    "react-infinite-scroller": "1.0.15",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-tap-event-plugin": "2.0.1",
    "react-test-renderer": "15.6.1",
    "redux": "3.7.2",
    "redux-form": "7.0.3",
    "redux-thunk": "2.2.0",
    "style-loader": "0.18.2",
    "styled-components": "2.1.2",
    "webpack": "3.5.5",
    "webpack-dev-server": "2.7.1",
    "webpack-merge": "4.1.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/test/localStorage.mock.js",
    "setupFiles": [
      "./src/test/jest.setup.js"
    ]
  }
}
